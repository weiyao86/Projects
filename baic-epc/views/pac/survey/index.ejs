<%
  viewBag.pageCode = 'survey';
  viewBag.cssFiles = ['pac_query'];
  viewBag.releaseCss = '';
%>

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="x-ua-compatible" content="IE=edge,chrome=1">
  	<title><%= pageTitle %></title>
  	<% if (viewBag.isLocal) { %>
    <link type="text/css" rel="stylesheet" href="/styles/frame.css" />
    <link type="text/css" rel="stylesheet" href="/styles/common.css" />
    <% viewBag.cssFiles.forEach(function (name) { %>
      <link type="text/css" rel="stylesheet" href="/styles/<%= name %>.css" />
    <% }) %>
  <% } else { %>
    <link type="text/css" rel="stylesheet" href="/release/styles/frame.min.css" />
    <link type="text/css" rel="stylesheet" href="/release/styles/common.min.css" />
    <link type="text/css" rel="stylesheet" href="/release/styles/<%= viewBag.releaseCss %>.css" />'
  <% } %>

  <% include ../../config/globalConfig.ejs %>
  <% include ../../config/requirejsConfig.ejs %>
</head>
<body>
	<div class="wrapper">
		<div class="header clearfix">
			<div class="page-related">
				EPC电子配件目录
			</div>
			<div class="arrow"></div>
		</div>
		<div class="content">
			<div class="logo_nav">
				<span class="logo"></span>
				<!--<a href="javascript:;" class="text-link big-text close-btn" id="close-btn">关闭</a>-->
			</div>
			<div class="title">
				<span class="title-text"><%=data.subject %></span>
				<div class="title-bg-car"></div>
			</div>
			<div class="detail">
				<div class="head-tip">
					<span>非常感谢您参与我们的问卷调查，调查的目的是为了我们更好的为你们服务！</span>
				</div>
				<div class="personal-info">
					<div class="data-item disabled">
						<span class="label short-label">
							<span class="text" title="账户名称">账户名称：</span>
						</span>
						<input class="input-box" type="text" data-name="usercode" value="<%=usercode %>" disabled="disabled">
						<span class="label">
							<span class="text" title="姓名">姓名：</span>
						</span>
						<input class="input-box" type="text" data-name="name">
						<span class="label required">
							<span class="text" title="手机">手机：</span>
						</span>
						<input class="input-box" type="text" data-name="phone" data-null="false">
					</div>
				</div>
				<form class="questions" id="question-wrapper" data-code="<%=data.code %>">
						
						<% data.questions.forEach(function(listitem){%>
							<div class="question-box required">
								<div class="label">
									<span><%=listitem.question%></span>
								</div>
								<textarea class="textarea-box" data-code="<%=listitem.code %>" data-null="false"></textarea>
							</div>
						<% }) %>
						

				</form>
				<div class="btn-wrapper">
					<a href="javascript:;" class="btn middle" data-name="submit">提交</a>
					<a href="javascript:;" class="btn middle secondary">取消</a>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="/scripts/bootstrap.js"></script>
</body>
</html>